{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "NLD Distance Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "nld-distance",
        // "data/training-models/Open-50-GE2E-bs256-M8-seed0",
        "data/training-models/Permute-50-GE2E-bs256-M8-seed0",
        "--debug"
      ]
    },
    {
      "name": "NLD GE2E Centroid",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["nld-save-ge2e-embedding-centroid", "--debug"]
    },
    {
      "name": "NLD Confidence Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "nld-confidence",
        "data/training-models/Open-20-CE-bs256-seed2",
        "--debug"
      ]
    },
    {
      "name": "Testing",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "test",
        "data/training-models/clean-CE-bs256-seed0",
        "--selected-iterations",
        "final",
        "--debug"
      ]
    },
    {
      "name": "Training",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "train",
        "AAMSC",
        "75",
        "Permute",
        "--N",
        "256",
        "--M",
        "1",
        "--s",
        "15",
        "--m",
        "0.1",
        "--K",
        "10",
        "--use-nld-result", "distance",
        "--random-seed", "0",
        // "--iteration",
        // "100",
        // "--debug"
      ]
    },
    {
      "name": "Process Mel Spectrogram",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["produce-mel-spectrogram"]
    },
    {
      "name": "Process Noisy Label",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["produce-noisy-label"]
    }
  ]
}
